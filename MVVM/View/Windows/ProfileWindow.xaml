<Window x:Class="Anime_List_App.MVVM.View.Windows.ProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anime_List_App.MVVM.View.Windows"
        mc:Ignorable="d"
       Title="ProfileWindow" Height="740" Width="1920" ResizeMode="CanResize"  Background="#eaecef">
    <StackPanel>
        <ContentPresenter x:Name="ToolBar"  Content="{Binding}" ContentTemplate="{StaticResource MainToolBar}"/>
        <Border Height="25"/>
        <Frame x:Name="ScreenFrame" NavigationUIVisibility="Hidden"/>
    </StackPanel>
</Window>
